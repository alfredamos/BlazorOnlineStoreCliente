@page "/orderList"
@inherits OrderListBase


<br />

<SearchItem OnSearchItem="HandleSearch" />

<br />

@if (Orders == null)
{
    Console.WriteLine("Load.....");
}
else if (Orders.Count == 0)
{
    Console.WriteLine("No order information is available at the moment.");
}
else
{
    <hr />

    <h1>All Customers Orders : </h1>

    <hr />

    @foreach (var order in Orders)
    {
        <OrderListOutput Order="order" Products="Products" ShowButton="true" OnOrderList="@(() => OrderByDateProcessed(order))"/>        
                     
    }

}

